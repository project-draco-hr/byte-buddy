{
  assertThat(describe(annotation).getValue(new MethodDescription.ForLoadedMethod(annotation.annotationType().getDeclaredMethod(methodName))),is(rawValue));
  assertThat(describe(annotation).getValue(new MethodDescription.Latent(new TypeDescription.ForLoadedType(annotation.annotationType()),methodName,new TypeDescription.ForLoadedType(annotation.annotationType().getDeclaredMethod(methodName).getReturnType()),Collections.<ParameterDescription.Token>emptyList(),Opcodes.ACC_PUBLIC,Collections.<GenericTypeDescription>emptyList(),Collections.<AnnotationDescription>emptyList())),is(rawValue));
  assertThat(annotation.annotationType().getDeclaredMethod(methodName).invoke(describe(annotation).prepare(annotation.annotationType()).load()),is(loadedValue));
}
