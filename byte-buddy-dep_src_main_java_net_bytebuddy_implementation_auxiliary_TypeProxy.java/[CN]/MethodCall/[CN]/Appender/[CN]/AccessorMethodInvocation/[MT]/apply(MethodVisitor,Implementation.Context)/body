{
  MethodDescription proxyMethod=methodAccessorFactory.registerAccessorFor(specialMethodInvocation);
  return new StackManipulation.Compound(MethodVariableAccess.REFERENCE.loadOffset(0),fieldLoadingInstruction,MethodVariableAccess.forBridgeMethodInvocation(instrumentedMethod,proxyMethod),MethodInvocation.invoke(proxyMethod),MethodReturn.returning(instrumentedMethod.getReturnType())).apply(methodVisitor,implementationContext);
}
