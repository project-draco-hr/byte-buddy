{
  TypeDescription proxyType=instrumentationContext.register(new TypeProxy(proxiedType,implementationTarget,InvocationFactory.Default.DEFAULT_METHOD,true,serializableProxy));
  return new Compound(TypeCreation.forType(proxyType),Duplication.SINGLE,MethodInvocation.invoke(proxyType.getDeclaredMethods().filter(isConstructor()).getOnly()),Duplication.SINGLE,MethodVariableAccess.forType(implementationTarget.getTypeDescription()).loadOffset(0),FieldAccess.forField(proxyType.getDeclaredFields().filter((named(INSTANCE_FIELD))).getOnly()).putter()).apply(methodVisitor,instrumentationContext);
}
