{
  ClassLoader classLoader=new ByteArrayClassLoader(null,ClassFileExtraction.of(PublicVisibilityBridgeExtension.class,VisibilityBridge.class,FooBar.class),null,ByteArrayClassLoader.PersistenceHandler.LATENT,PackageDefiner.NoOp.INSTANCE);
  Class<?> type=new ByteBuddy().subclass(PublicVisibilityBridgeExtension.class).modifiers(Opcodes.ACC_PUBLIC).make().load(classLoader,ClassLoadingStrategy.Default.INJECTION).getLoaded();
  assertThat(type.getDeclaredConstructors().length,is(1));
  assertThat(type.getDeclaredMethods().length,is(0));
}
