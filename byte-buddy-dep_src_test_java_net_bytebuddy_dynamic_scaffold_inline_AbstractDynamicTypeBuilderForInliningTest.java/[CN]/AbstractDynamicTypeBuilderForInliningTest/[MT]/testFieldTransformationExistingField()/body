{
  Class<?> type=create(Transform.class).field(named(FOO)).transform(FieldTransformer.Simple.withModifiers(Visibility.PUBLIC)).make().load(new URLClassLoader(new URL[0],null),ClassLoadingStrategy.Default.WRAPPER).getLoaded();
  assertThat(type.getDeclaredField(FOO).getModifiers(),is(Opcodes.ACC_PUBLIC));
}
