{
  doReturn(FirstPseudoAnnotation.class).when(firstArgumentBinder).getHandledType();
  doReturn(FirstPseudoAnnotation.class).when(secondArgumentBinder).getHandledType();
  new AnnotationDrivenBinder(Arrays.<AnnotationDrivenBinder.ArgumentBinder<?>>asList(firstArgumentBinder,secondArgumentBinder),defaultsProvider,assigner,methodInvoker);
}
