{
  TypeWriter.FieldPool.Record record=new FieldRegistry.Default().compile(instrumentedType).target(unknownField);
  assertThat(record.resolveDefault(defaultValue),is(defaultValue));
  assertThat(record.getFieldAppender(),is((FieldAttributeAppender)new FieldAttributeAppender.ForField(unknownField,AnnotationAppender.ValueFilter.AppendDefaults.INSTANCE)));
}
