{
  ObjectPropertyAssertion.of(MethodVariableAccess.ArgumentLoadingStackManipulation.class).apply();
  MethodDescription methodDescription=mock(MethodDescription.class);
  when(methodDescription.getParameters()).thenReturn(new ParameterList.Empty());
  ObjectPropertyAssertion.of(MethodVariableAccess.TypeCastingHandler.ForBridgeTarget.class).apply(new MethodVariableAccess.TypeCastingHandler.ForBridgeTarget(methodDescription));
}
