{
  if (!sourceType.isPrimitive() && targetType.represents(String.class)) {
    MethodDescription toStringMethod=new TypeDescription.ForLoadedType(Object.class).getDeclaredMethods().filter(named("toString")).getOnly();
    return MethodInvocation.invoke(toStringMethod).virtual(sourceType);
  }
 else {
    return StackManipulation.Illegal.INSTANCE;
  }
}
