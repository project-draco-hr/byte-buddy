{
  TypePool typePool=TypePool.Default.ofClassPath();
  new ByteBuddy().redefine(typePool.describe(getClass().getName() + "$UnloadedBar").resolve(),ClassFileLocator.ForClassLoader.of(getClass().getClassLoader())).defineField("qux",String.class).make().load(ClassLoader.getSystemClassLoader(),ClassLoadingStrategy.Default.INJECTION);
  assertThat(UnloadedBar.class.getDeclaredField("qux"),notNullValue(java.lang.reflect.Field.class));
}
