{
  Object object=new ByteBuddy().subclass(Object.class,ConstructorStrategy.Default.NO_CONSTRUCTORS).defineConstructor(Collections.singletonList(int.class),Visibility.PUBLIC).intercept(MethodCall.invoke(Object.class.getDeclaredConstructor())).make().load(getClass().getClassLoader(),ClassLoadingStrategy.Default.WRAPPER).getLoaded().getDeclaredConstructor(int.class).newInstance(42);
  assertNotEquals(Object.class,object.getClass());
}
