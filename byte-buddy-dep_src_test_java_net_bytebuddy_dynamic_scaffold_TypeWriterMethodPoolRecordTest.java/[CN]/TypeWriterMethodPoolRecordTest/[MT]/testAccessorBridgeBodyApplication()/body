{
  new TypeWriter.MethodPool.Record.AccessBridgeWrapper(delegate,instrumentedType,bridgeTarget,Collections.singleton(typeToken),methodAttributeAppender).applyBody(methodVisitor,implementationContext,valueFilterFactory);
  verify(delegate).applyBody(methodVisitor,implementationContext,valueFilterFactory);
  verifyNoMoreInteractions(delegate);
  verifyZeroInteractions(methodVisitor);
}
