{
  if (!methodDescription.isTypeInitializer()) {
    throw new IllegalArgumentException(this + " must only be applied to a type initializer");
  }
  Entry wrappedEntry=wrappedMethodPool.target(methodDescription);
  if (registeredFieldCacheEntries.isEmpty()) {
    return wrappedEntry;
  }
 else   if (!wrappedEntry.isDefineMethod()) {
    return new Entry.Simple(new ByteCodeAppender.Compound(this,TypeInitializerReturn.INSTANCE),MethodAttributeAppender.NoOp.INSTANCE);
  }
 else {
    return new Entry.Simple(new ByteCodeAppender.Compound(this,wrappedEntry.getByteCodeAppender()),wrappedEntry.getAttributeAppender());
  }
}
