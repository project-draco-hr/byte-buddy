{
  DynamicType.Loaded<?> loaded=implement(Object.class,MethodCall.invoke(classLoader.loadClass(SINGLE_DEFAULT_METHOD).getDeclaredMethod(FOO)).onDefault(),classLoader,ElementMatchers.isMethod().and(ElementMatchers.not(isDeclaredBy(Object.class))),classLoader.loadClass(SINGLE_DEFAULT_METHOD));
  assertThat(loaded.getLoaded().getDeclaredMethods().length,is(1));
  Method method=loaded.getLoaded().getDeclaredMethod(FOO);
  Object instance=loaded.getLoaded().newInstance();
  assertThat(method.invoke(instance),is((Object)FOO));
}
