{
  DynamicType.Loaded<?> loaded=instrument(Object.class,DefaultMethodCall.preferring(preferredInterfaces),classLoader,not(isDeclaredBy(Object.class)),preferredInterface,secondInterface);
  assertThat(loaded.getLoaded().getDeclaredMethods().length,is(1));
  Method method=loaded.getLoaded().getDeclaredMethod(FOO);
  Object instance=loaded.getLoaded().newInstance();
  assertThat(method.invoke(instance),is(expectedResult));
}
