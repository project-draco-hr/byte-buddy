{
  DynamicType.Loaded<?> loaded=instrument(classLoader.loadClass(SINGLE_DEFAULT_METHOD_CLASS),DefaultMethodCall.unambiguousOnly(),classLoader,isMethod().and(not(isDeclaredBy(Object.class))),classLoader.loadClass(SINGLE_DEFAULT_METHOD));
  assertThat(loaded.getLoaded().getDeclaredMethods().length,is(1));
  Method method=loaded.getLoaded().getDeclaredMethod(FOO);
  Object instance=loaded.getLoaded().newInstance();
  assertThat(method.invoke(instance),is((Object)FOO));
}
