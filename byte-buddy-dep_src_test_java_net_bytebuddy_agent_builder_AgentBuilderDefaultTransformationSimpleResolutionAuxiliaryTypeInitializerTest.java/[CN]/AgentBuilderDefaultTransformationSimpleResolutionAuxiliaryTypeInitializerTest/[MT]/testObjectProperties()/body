{
  ObjectPropertyAssertion.of(AgentBuilder.Default.Transformation.Simple.Resolution.AuxiliaryTypeInitializer.class).create(new ObjectPropertyAssertion.Creator<AccessControlContext>(){
    @Override public AccessControlContext create(){
      return new AccessControlContext(new ProtectionDomain[]{mock(ProtectionDomain.class)});
    }
  }
).apply();
