{
  return new MethodDelegation(new ImplementationDelegate.ForInstanceField(nonNull(typeDescription),isValidIdentifier(fieldName)),defaultParameterBinders(),defaultDefaultsProvider(),TargetMethodAnnotationDrivenBinder.TerminationHandler.Returning.INSTANCE,defaultAmbiguityResolver(),Assigner.DEFAULT,methodLookupEngine.process(typeDescription).getInvokableMethods().filter(not(isStatic().or(isPrivate()).or(isConstructor()))));
}
