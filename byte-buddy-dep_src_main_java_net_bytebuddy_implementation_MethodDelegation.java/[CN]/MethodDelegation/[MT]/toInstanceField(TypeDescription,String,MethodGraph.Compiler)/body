{
  return new MethodDelegation(new ImplementationDelegate.ForInstanceField(nonNull(typeDescription),isValidIdentifier(fieldName)),defaultParameterBinders(),defaultDefaultsProvider(),TargetMethodAnnotationDrivenBinder.TerminationHandler.Returning.INSTANCE,defaultAmbiguityResolver(),Assigner.DEFAULT,methodGraphCompiler.compile(typeDescription).listNodes().filter(not(isStatic().or(isPrivate()).or(isConstructor()))).asMethodList());
}
