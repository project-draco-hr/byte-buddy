{
  StringBuilder toString=new StringBuilder();
  toString.append('@');
  toString.append(annotationType.getName());
  toString.append('(');
  boolean firstMember=true;
  for (  Map.Entry<Method,AnnotationDescription.AnnotationValue.Loaded<?>> entry : values.entrySet()) {
    if (!entry.getValue().getState().isDefined()) {
      continue;
    }
    if (firstMember) {
      firstMember=false;
    }
 else {
      toString.append(", ");
    }
    toString.append(entry.getKey().getName()).append('=').append(entry.getValue().toString());
  }
  toString.append(')');
  return toString.toString();
}
