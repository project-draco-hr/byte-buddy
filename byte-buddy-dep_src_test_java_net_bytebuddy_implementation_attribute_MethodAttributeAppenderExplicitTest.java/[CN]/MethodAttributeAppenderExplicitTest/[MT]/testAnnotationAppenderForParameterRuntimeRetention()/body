{
  new MethodAttributeAppender.Explicit(PARAMETER_INDEX,new AnnotationList.ForLoadedAnnotation(new Baz.Instance())).apply(methodVisitor,methodDescription,annotationValueFilter);
  verify(methodVisitor).visitParameterAnnotation(PARAMETER_INDEX,Type.getDescriptor(Baz.class),true);
  verifyNoMoreInteractions(methodVisitor);
  verify(methodDescription).getParameters();
  verifyNoMoreInteractions(methodDescription);
}
