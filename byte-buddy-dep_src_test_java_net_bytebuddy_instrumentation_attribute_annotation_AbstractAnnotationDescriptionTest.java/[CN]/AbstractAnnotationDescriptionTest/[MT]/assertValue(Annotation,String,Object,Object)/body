{
  assertThat(describe(annotation).getValue(new MethodDescription.ForLoadedMethod(annotation.annotationType().getDeclaredMethod(methodName))),is(rawValue));
  assertThat(describe(annotation).getValue(new MethodDescription.Latent(methodName,new TypeDescription.ForLoadedType(annotation.annotationType()),new TypeDescription.ForLoadedType(annotation.annotationType().getDeclaredMethod(methodName).getReturnType()),new TypeList.Empty(),Opcodes.ACC_PUBLIC,new TypeList.Empty())),is(rawValue));
  assertThat(annotation.annotationType().getDeclaredMethod(methodName).invoke(describe(annotation).prepare(annotation.annotationType()).load()),is(loadedValue));
}
