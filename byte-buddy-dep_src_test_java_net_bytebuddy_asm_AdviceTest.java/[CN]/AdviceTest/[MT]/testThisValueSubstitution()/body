{
  Class<?> type=new ByteBuddy().redefine(Box.class).visit(Advice.to(ThisSubstitutionAdvice.class).on(named(FOO))).make().load(ClassLoadingStrategy.BOOTSTRAP_LOADER,ClassLoadingStrategy.Default.WRAPPER).getLoaded();
  assertThat(type.getDeclaredMethod(FOO).invoke(type.getDeclaredConstructor(String.class).newInstance(FOO)),is((Object)BAR));
}
