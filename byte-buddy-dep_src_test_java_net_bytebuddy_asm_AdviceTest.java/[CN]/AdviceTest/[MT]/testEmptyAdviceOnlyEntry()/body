{
  Class<?> type=new ByteBuddy().redefine(EmptyMethod.class).visit(DebuggingWrapper.makeDefault()).visit(Advice.to(EmptyAdviceEntry.class).on(named(FOO)).readerFlags(ClassReader.SKIP_DEBUG)).make().load(null,ClassLoadingStrategy.Default.WRAPPER).getLoaded();
  assertThat(type.getDeclaredMethod(FOO).invoke(type.newInstance()),nullValue(Object.class));
}
