{
  Class<?> type=new ByteBuddy().redefine(Sample.class).visit(Advice.to(ExceptionHandlerAdvice.class).on(named(FOO))).visit(Advice.to(ExceptionHandlerAdvice.class).on(named(FOO))).make().load(ClassLoadingStrategy.BOOTSTRAP_LOADER,ClassLoadingStrategy.Default.WRAPPER).getLoaded();
  assertThat(type.getDeclaredMethod(FOO).invoke(type.getDeclaredConstructor().newInstance()),is((Object)FOO));
  assertThat(type.getDeclaredField(ENTER).get(null),is((Object)2));
  assertThat(type.getDeclaredField(EXIT).get(null),is((Object)2));
}
