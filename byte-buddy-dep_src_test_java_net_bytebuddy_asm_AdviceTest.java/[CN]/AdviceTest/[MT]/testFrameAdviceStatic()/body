{
  Class<?> type=new ByteBuddy().redefine(FrameSample.class).visit(Advice.to(FrameAdvice.class).on(named(BAR))).make().load(null,ClassLoadingStrategy.Default.WRAPPER).getLoaded();
  assertThat(type.getDeclaredMethod(BAR,String.class).invoke(null,FOO),is((Object)FOO));
  assertThat(type.getField(COUNT).getInt(null),is((Object)2));
}
