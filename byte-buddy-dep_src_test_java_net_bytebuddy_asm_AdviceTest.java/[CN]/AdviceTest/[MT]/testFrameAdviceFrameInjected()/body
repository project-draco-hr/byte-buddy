{
  Class<?> type=new ByteBuddy().redefine(Sample.class).visit(DebuggingWrapper.makeDefault()).visit(Advice.to(FrameExitAdvice.class).on(named(FOO))).make().load(null,ClassLoadingStrategy.Default.WRAPPER).getLoaded();
  assertThat(type.getDeclaredMethod(FOO).invoke(type.newInstance()),is((Object)FOO));
}
