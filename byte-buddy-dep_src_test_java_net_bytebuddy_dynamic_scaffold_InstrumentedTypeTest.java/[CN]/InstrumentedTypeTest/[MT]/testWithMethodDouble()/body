{
  TypeDescription returnType=mock(TypeDescription.class);
  when(returnType.asRawType()).thenReturn(returnType);
  when(returnType.getName()).thenReturn(FOO);
  when(returnType.accept(Mockito.any(GenericTypeDescription.Visitor.class))).thenReturn(returnType);
  makePlainInstrumentedType().withMethod(new MethodDescription.Token(BAR,Opcodes.ACC_PUBLIC,returnType,Collections.<TypeDescription>emptyList())).withMethod(new MethodDescription.Token(BAR,Opcodes.ACC_PUBLIC,returnType,Collections.<TypeDescription>emptyList()));
}
