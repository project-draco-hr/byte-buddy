{
  StackManipulation.Size stackSize=new StackManipulation.Compound(MethodVariableAccess.loadAll(instrumentedMethod),MethodInvocation.invoke(instrumentedMethod).special(targetType),MethodReturn.returning(instrumentedMethod.getReturnType())).apply(methodVisitor,instrumentationContext);
  return new Size(stackSize.getMaximalSize(),instrumentedMethod.getStackSize());
}
