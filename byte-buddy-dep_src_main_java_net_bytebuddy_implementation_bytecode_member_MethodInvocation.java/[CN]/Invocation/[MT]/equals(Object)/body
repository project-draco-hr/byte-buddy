{
  if (this == other)   return true;
  if (other == null || getClass() != other.getClass())   return false;
  Invocation that=(Invocation)other;
  return MethodInvocation.this.equals(((Invocation)other).getOuterInstance()) && methodDescription.asToken().equals(that.methodDescription.asToken()) && typeDescription.equals(that.typeDescription);
}
