{
  Class<?> type=createPlain().defineField(QUX,String.class).defineField(QUX,Object.class).make().load(getClass().getClassLoader(),ClassLoadingStrategy.Default.CHILD_FIRST).getLoaded();
  for (  Field field : type.getDeclaredFields()) {
    if (field.getType() == String.class) {
      assertThat(field.getName(),is(QUX));
    }
 else     if (field.getType() == Object.class) {
      assertThat(field.getName(),is(QUX));
    }
 else {
      throw new AssertionError();
    }
  }
}
