{
  Class<?> type=createPlain().defineMethod(FOO,Object.class,Collections.<Class<?>>emptyList(),Visibility.PUBLIC).intercept(new Implementation.Simple(new TextConstant(FOO),MethodReturn.REFERENCE)).make().load(new URLClassLoader(new URL[0],null),ClassLoadingStrategy.Default.WRAPPER).getLoaded();
  Method method=type.getDeclaredMethod(FOO);
  assertThat(method.invoke(type.newInstance()),is((Object)FOO));
}
