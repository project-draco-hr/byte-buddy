{
  return new SubclassDynamicTypeBuilder<S>(classFileVersion,namingStrategy,superType,interfaceTypes,modifiers,attributeAppender,ignoredMethods,bridgeMethodResolverFactory,classVisitorWrapperChain,fieldRegistry.include(fieldToken,attributeAppenderFactory),methodRegistry,defaultFieldAttributeAppenderFactory,defaultMethodAttributeAppenderFactory,join(fieldTokens,fieldToken),methodTokens);
}
