{
  DynamicType.Loaded<?> loaded=implement(Object.class,MethodDelegation.to(Class.forName(PREFERRING_INTERCEPTOR)),getClass().getClassLoader(),isMethod().and(not(isDeclaredBy(Object.class))),Class.forName(SINGLE_DEFAULT_METHOD),Class.forName(CONFLICTING_INTERFACE));
  Object instance=loaded.getLoaded().getDeclaredConstructor().newInstance();
  Method method=loaded.getLoaded().getMethod(FOO);
  assertThat(method.invoke(instance),is((Object)FOO));
}
