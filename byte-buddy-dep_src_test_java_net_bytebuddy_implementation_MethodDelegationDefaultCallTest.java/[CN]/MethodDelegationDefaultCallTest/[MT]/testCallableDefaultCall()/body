{
  DynamicType.Loaded<?> loaded=implement(Object.class,MethodDelegation.to(CallableClass.class),classLoader,isMethod().and(not(isDeclaredBy(Object.class))),classLoader.loadClass(SINGLE_DEFAULT_METHOD));
  Object instance=loaded.getLoaded().newInstance();
  Method method=loaded.getLoaded().getMethod(FOO);
  assertThat(method.invoke(instance),is((Object)FOO));
}
