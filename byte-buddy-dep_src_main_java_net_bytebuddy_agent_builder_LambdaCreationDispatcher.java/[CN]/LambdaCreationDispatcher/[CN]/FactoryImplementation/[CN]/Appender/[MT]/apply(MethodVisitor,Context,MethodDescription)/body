{
  return new Size(new StackManipulation.Compound(MethodVariableAccess.allArgumentsOf(instrumentedMethod),TypeCreation.of(instrumentedType),Duplication.SINGLE,MethodInvocation.invoke(instrumentedType.getDeclaredMethods().filter(isConstructor()).getOnly()),MethodReturn.REFERENCE).apply(methodVisitor,implementationContext).getMaximalSize(),instrumentedMethod.getStackSize());
}
