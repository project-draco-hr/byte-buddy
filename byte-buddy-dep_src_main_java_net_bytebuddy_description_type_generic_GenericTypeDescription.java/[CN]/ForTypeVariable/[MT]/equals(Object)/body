{
  if (!(other instanceof GenericTypeDescription))   return false;
  GenericTypeDescription genericTypeDescription=(GenericTypeDescription)other;
  return genericTypeDescription.getSort().isTypeVariable() && !genericTypeDescription.getSort().isDetachedTypeVariable() && getSymbol().equals(genericTypeDescription.getSymbol())&& getVariableSource().equals(genericTypeDescription.getVariableSource());
}
