{
  TypeDescription.Generic typeDescription=mock(TypeDescription.Generic.class);
  when(typeDescription.getSort()).thenReturn(TypeDefinition.Sort.NON_GENERIC);
  assertThat(TypeDescription.Generic.OfGenericArray.Latent.of(typeDescription,1,Collections.emptyList()).getSort(),is(TypeDefinition.Sort.NON_GENERIC));
  assertThat(TypeDescription.Generic.OfGenericArray.Latent.of(typeDescription,1,Collections.emptyList()).getComponentType(),is(typeDescription));
}
