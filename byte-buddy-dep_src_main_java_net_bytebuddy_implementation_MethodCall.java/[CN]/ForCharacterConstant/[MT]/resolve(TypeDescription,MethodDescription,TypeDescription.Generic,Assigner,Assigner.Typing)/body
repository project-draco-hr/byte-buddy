{
  StackManipulation stackManipulation=new StackManipulation.Compound(IntegerConstant.forValue(value),assigner.assign(new TypeDescription.Generic.OfNonGenericType.ForLoadedType(char.class),target,typing));
  if (!stackManipulation.isValid()) {
    throw new IllegalStateException("Cannot assign char value to " + target);
  }
  return stackManipulation;
}
