{
  StackManipulation stackManipulation=new StackManipulation.Compound(IntegerConstant.forValue(value),assigner.assign(new TypeDescription.ForLoadedType(int.class),targetType,typing));
  if (!stackManipulation.isValid()) {
    throw new IllegalStateException("Cannot assign integer value to " + targetType);
  }
  return stackManipulation;
}
