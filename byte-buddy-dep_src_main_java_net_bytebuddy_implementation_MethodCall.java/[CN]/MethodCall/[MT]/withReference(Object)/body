{
  List<ArgumentLoader> argumentLoaders=new ArrayList<ArgumentLoader>(argument.length);
  for (  Object anArgument : argument) {
    argumentLoaders.add(anArgument == null ? ArgumentLoader.ForNullConstant.INSTANCE : new ArgumentLoader.ForStaticField(anArgument));
  }
  return new MethodCall(methodLocator,targetHandler,join(this.argumentLoaders,argumentLoaders),methodInvoker,terminationHandler,assigner,typing);
}
