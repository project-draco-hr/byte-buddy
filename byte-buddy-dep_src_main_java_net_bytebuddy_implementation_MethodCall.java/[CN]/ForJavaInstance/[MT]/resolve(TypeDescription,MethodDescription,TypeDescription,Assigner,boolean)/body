{
  StackManipulation stackManipulation=new StackManipulation.Compound(javaInstance.asStackManipulation(),assigner.assign(javaInstance.getInstanceType(),targetType,dynamicallyTyped));
  if (!stackManipulation.isValid()) {
    throw new IllegalStateException("Cannot assign Class value to " + targetType);
  }
  return stackManipulation;
}
