{
  StackManipulation stackManipulation=new StackManipulation.Compound(javaInstance.asStackManipulation(),assigner.assign(javaInstance.getInstanceType().asGenericType(),target,typing));
  if (!stackManipulation.isValid()) {
    throw new IllegalStateException("Cannot assign Class value to " + target);
  }
  return stackManipulation;
}
