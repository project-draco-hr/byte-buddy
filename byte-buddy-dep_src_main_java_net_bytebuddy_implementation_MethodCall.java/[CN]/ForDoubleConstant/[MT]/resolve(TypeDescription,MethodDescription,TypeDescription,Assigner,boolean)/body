{
  StackManipulation stackManipulation=new StackManipulation.Compound(DoubleConstant.forValue(value),assigner.assign(new TypeDescription.ForLoadedType(double.class),targetType,dynamicallyTyped));
  if (!stackManipulation.isValid()) {
    throw new IllegalStateException("Cannot assign double value to " + targetType);
  }
  return stackManipulation;
}
