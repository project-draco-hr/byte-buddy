{
  StackManipulation stackManipulation=new StackManipulation.Compound(FieldAccess.forEnumeration(enumerationDescription),assigner.assign(enumerationDescription.getEnumerationType(),targetType,dynamicallyTyped));
  if (!stackManipulation.isValid()) {
    throw new IllegalStateException("Cannot assign " + enumerationDescription.getEnumerationType() + " value to "+ targetType);
  }
  return stackManipulation;
}
