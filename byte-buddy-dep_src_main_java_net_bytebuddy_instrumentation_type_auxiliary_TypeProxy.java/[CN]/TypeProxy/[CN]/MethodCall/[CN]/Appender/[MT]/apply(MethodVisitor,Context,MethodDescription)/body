{
  MethodDescription proxyMethod=methodAccessorFactory.registerAccessorFor(instrumentationTarget.invokeSuper(instrumentedMethod,Target.MethodLookup.RESOLVE_BRIDGES));
  StackManipulation.Size stackSize=new StackManipulation.Compound(MethodVariableAccess.forType(instrumentationTarget.getTypeDescription()).loadFromIndex(0),fieldLoadingInstruction,MethodVariableAccess.loadArguments(instrumentedMethod),MethodInvocation.invoke(proxyMethod),MethodReturn.returning(instrumentedMethod.getReturnType())).apply(methodVisitor,instrumentationContext);
  return new Size(stackSize.getMaximalSize(),instrumentedMethod.getStackSize());
}
