{
  return new ByteBuddy(classFileVersion).withIgnoredMethods(ignoreFinalizer ? isFinalizer() : none()).subclass(proxiedType).name(auxiliaryTypeName).modifiers(DEFAULT_TYPE_MODIFIER).methodLookupEngine(this).implement(serializableProxy ? new Class<?>[]{Serializable.class} : new Class<?>[0]).method(any()).intercept(new MethodCall(methodAccessorFactory)).defineMethod(REFLECTION_METHOD,TargetType.DESCRIPTION,Collections.<TypeDescription>emptyList(),Ownership.STATIC).intercept(SilentConstruction.INSTANCE).make();
}
