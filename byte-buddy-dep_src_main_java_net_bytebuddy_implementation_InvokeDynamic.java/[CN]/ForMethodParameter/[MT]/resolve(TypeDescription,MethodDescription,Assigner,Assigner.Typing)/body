{
  ParameterList<?> parameters=instrumentedMethod.getParameters();
  if (index >= parameters.size()) {
    throw new IllegalStateException("No parameter " + index + " for "+ instrumentedMethod);
  }
  return new Resolved.Simple(MethodVariableAccess.forType(parameters.get(index).getType().asErasure()).loadOffset(instrumentedMethod.getParameters().get(index).getOffset()),parameters.get(index).getType().asErasure());
}
