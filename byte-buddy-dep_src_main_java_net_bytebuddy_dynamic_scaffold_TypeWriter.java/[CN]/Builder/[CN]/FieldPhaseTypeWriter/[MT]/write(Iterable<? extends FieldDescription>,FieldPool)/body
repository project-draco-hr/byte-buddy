{
  for (  FieldDescription fieldDescription : fieldDescriptions) {
    FieldVisitor fieldVisitor=classVisitor.visitField(fieldDescription.getModifiers(),fieldDescription.getInternalName(),fieldDescription.getDescriptor(),null,null);
    fieldPool.target(fieldDescription).getFieldAppenderFactory().make(instrumentedType).apply(fieldVisitor,fieldDescription);
    fieldVisitor.visitEnd();
  }
  return this;
}
