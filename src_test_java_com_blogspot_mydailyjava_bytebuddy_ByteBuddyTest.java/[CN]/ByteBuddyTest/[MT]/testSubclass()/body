{
  Object object=ByteBuddy.make().withAppendedClassVisitorWrapper(new DebuggingWrapper(System.out)).subclass(Bar.class).method(MethodMatchers.returns(String.class)).intercept(SuperClassDelegation.INSTANCE).make().load(getClass().getClassLoader()).newInstance();
  System.out.println(Arrays.asList(object.getClass().getDeclaredMethods()));
  System.out.println(object.getClass());
  System.out.println(object.getClass().getSuperclass());
  System.out.println(object.toString());
}
