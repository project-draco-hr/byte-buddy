{
  Foo foo=new ByteBuddy().subclass(Xyz.class,ConstructorStrategy.Default.IMITATE_SUPER_TYPE).method(isDeclaredBy(Xyz.class)).intercept(MethodDelegation.to(Qux.class)).make().load(getClass().getClassLoader(),ClassLoadingStrategy.Default.WRAPPER).getLoaded().newInstance();
  System.out.println(foo.bar());
}
