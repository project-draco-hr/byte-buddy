{
  assertThat(new TypePool.LazyTypeDescription.DeclarationContext.DeclaredInMethod(FOO_INTERNAL,BAR,QUX).isDeclaredInMethod(),is(true));
  assertThat(new TypePool.LazyTypeDescription.DeclarationContext.DeclaredInMethod(FOO_INTERNAL,BAR,QUX).isDeclaredInType(),is(false));
  assertThat(new TypePool.LazyTypeDescription.DeclarationContext.DeclaredInMethod(FOO_INTERNAL,BAR,QUX).isSelfDeclared(),is(false));
}
