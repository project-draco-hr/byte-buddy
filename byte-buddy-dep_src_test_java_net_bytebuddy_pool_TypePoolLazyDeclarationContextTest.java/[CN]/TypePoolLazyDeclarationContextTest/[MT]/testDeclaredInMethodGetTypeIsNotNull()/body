{
  TypeDescription typeDescription=mock(TypeDescription.class);
  TypePool typePool=mock(TypePool.class);
  when(typePool.describe(FOO)).thenReturn(new TypePool.Resolution.Simple(typeDescription));
  assertThat(new TypePool.LazyTypeDescription.DeclarationContext.DeclaredInMethod(FOO_INTERNAL,BAR,QUX).getEnclosingType(typePool),is(typeDescription));
}
