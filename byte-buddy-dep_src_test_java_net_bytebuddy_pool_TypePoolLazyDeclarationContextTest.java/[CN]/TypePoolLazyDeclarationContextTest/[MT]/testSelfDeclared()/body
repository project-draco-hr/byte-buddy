{
  assertThat(TypePool.LazyTypeDescription.DeclarationContext.SelfDeclared.INSTANCE.isDeclaredInMethod(),is(false));
  assertThat(TypePool.LazyTypeDescription.DeclarationContext.SelfDeclared.INSTANCE.isDeclaredInType(),is(false));
  assertThat(TypePool.LazyTypeDescription.DeclarationContext.SelfDeclared.INSTANCE.isSelfDeclared(),is(true));
}
