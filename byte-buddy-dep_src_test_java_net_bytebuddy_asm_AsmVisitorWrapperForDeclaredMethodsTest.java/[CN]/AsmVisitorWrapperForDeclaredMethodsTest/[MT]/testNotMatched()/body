{
  assertThat(new AsmVisitorWrapper.ForDeclaredMethods().method(matcher,methodVisitorWrapper).wrap(instrumentedType,classVisitor).visitMethod(MODIFIERS,BAR,BAZ,BAZ,new String[]{QUX + BAZ}),is(methodVisitor));
  verify(matcher).matches(bar);
  verifyNoMoreInteractions(matcher);
  verifyZeroInteractions(methodVisitorWrapper);
}
