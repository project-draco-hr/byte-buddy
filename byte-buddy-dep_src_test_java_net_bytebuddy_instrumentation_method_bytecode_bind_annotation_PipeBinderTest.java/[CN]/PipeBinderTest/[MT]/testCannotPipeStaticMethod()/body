{
  when(targetTypeList.get(0)).thenReturn(targetMethodType);
  when(source.isStatic()).thenReturn(true);
  MethodDelegationBinder.ParameterBinding<?> parameterBinding=binder.bind(annotationDescription,0,source,target,instrumentationTarget,assigner);
  assertThat(parameterBinding.isValid(),is(false));
}
