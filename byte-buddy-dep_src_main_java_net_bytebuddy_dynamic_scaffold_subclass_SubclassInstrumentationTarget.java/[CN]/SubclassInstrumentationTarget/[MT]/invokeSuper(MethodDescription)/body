{
  if (methodDescription.isConstructor()) {
    methodDescription=this.superConstructors.get(methodDescription.getParameterTypes());
    if (methodDescription == null) {
      return Instrumentation.SpecialMethodInvocation.Illegal.INSTANCE;
    }
  }
  return methodDescription.isSpecializableFor(typeDescription.getSupertype()) ? new Instrumentation.SpecialMethodInvocation.Legal(methodDescription,typeDescription.getSupertype()) : Instrumentation.SpecialMethodInvocation.Illegal.INSTANCE;
}
