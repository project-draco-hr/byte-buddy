{
  TypeDescription auxiliaryType=instrumentationContext.register(new MethodCallProxy(targetMethod));
  return new Compound(TypeCreation.forType(auxiliaryType),Duplication.SINGLE,MethodVariableAccess.loadAll(targetMethod),MethodInvocation.invoke(auxiliaryType.getDeclaredMethods().filter(isConstructor()).getOnly())).apply(methodVisitor,instrumentationContext);
}
