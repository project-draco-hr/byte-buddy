{
  DynamicType.Loaded<?> loaded=implement(Object.class,MethodDelegation.to(Class.forName(DEFAULT_INTERFACE_TARGET_IMPLICIT)).appendParameterBinder(Morph.Binder.install(Morphing.class)),getClass().getClassLoader(),isMethod().and(not(isDeclaredBy(Object.class))),Class.forName(DEFAULT_INTERFACE));
  Object instance=loaded.getLoaded().newInstance();
  assertThat(instance.getClass().getDeclaredMethod(FOO,String.class).invoke(instance,QUX),is((Object)(FOO + BAR)));
}
