{
  TypeWriter.FieldPool.Record record=new TypeWriter.FieldPool.Record.ForSimpleField(fieldDescription);
  assertThat(record.getFieldAppender(),is((FieldAttributeAppender)new FieldAttributeAppender.ForField(fieldDescription,AnnotationAppender.ValueFilter.AppendDefaults.INSTANCE)));
  assertThat(record.resolveDefault(defaultValue),is(defaultValue));
}
