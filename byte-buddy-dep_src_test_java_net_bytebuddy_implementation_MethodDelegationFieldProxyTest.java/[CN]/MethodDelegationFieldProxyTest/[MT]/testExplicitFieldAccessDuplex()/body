{
  DynamicType.Loaded<Explicit> loaded=implement(Explicit.class,MethodDelegation.to(SwapDuplex.class).appendParameterBinder(FieldProxy.Binder.install(GetSet.class)));
  Explicit explicit=loaded.getLoaded().getDeclaredConstructor().newInstance();
  assertThat(explicit.foo,is(FOO));
  explicit.swap();
  assertThat(explicit.foo,is(FOO + BAR));
}
