{
  Size argumentSize=prepare(methodVisitor).aggregate(ArrayFactory.forType(TypeDescription.CLASS).withValues(typeConstantsFor(methodDescription.getParameters().asTypeList().asErasures())).apply(methodVisitor,implementationContext));
  methodVisitor.visitMethodInsn(Opcodes.INVOKEVIRTUAL,CLASS_TYPE_INTERNAL_NAME,getMethodName(),getDescriptor(),false);
  return new Size(1,argumentSize.getMaximalSize());
}
