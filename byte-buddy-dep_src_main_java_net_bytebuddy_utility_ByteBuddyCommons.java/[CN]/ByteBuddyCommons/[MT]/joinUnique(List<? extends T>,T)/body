{
  List<T> result=new ArrayList<T>(list.size() + 1);
  for (  T listElement : list) {
    if (listElement.equals(element)) {
      throw new IllegalArgumentException("Conflicting elements: " + listElement + " and "+ element);
    }
    result.add(listElement);
  }
  result.add(element);
  return result;
}
