{
  if (module != null && module.isNamed()) {
    for (    JavaModule target : modules) {
      if (!module.canRead(target)) {
        module.addReads(instrumentation,target);
      }
      if (addTargetEdge && !target.canRead(module)) {
        target.addReads(instrumentation,module);
      }
    }
  }
}
