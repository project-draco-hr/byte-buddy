{
  if (this == other)   return true;
  if (other == null || getClass() != other.getClass())   return false;
  Appender appender=(Appender)other;
  return targetMethod.equals(appender.targetMethod) && declaredFields.equals(appender.declaredFields) && specializedLambdaMethod.equals(appender.specializedLambdaMethod);
}
