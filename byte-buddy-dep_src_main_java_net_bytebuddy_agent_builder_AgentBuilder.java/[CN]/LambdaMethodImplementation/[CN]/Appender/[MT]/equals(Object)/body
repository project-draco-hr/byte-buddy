{
  if (this == other)   return true;
  if (other == null || getClass() != other.getClass())   return false;
  Appender appender=(Appender)other;
  return lambdaDispatcherMethod.equals(appender.lambdaDispatcherMethod) && declaredFields.equals(appender.declaredFields) && specializedLambdaMethod.equals(appender.specializedLambdaMethod);
}
