{
  if (JavaModule.isSupported()) {
    Set<JavaModule> modules=new HashSet<JavaModule>();
    for (    Class<?> aType : type) {
      JavaModule module=JavaModule.ofType(aType);
      if (module.isNamed()) {
        modules.add(module);
      }
    }
    return modules.isEmpty() ? this : with(new Listener.ModuleReadEdgeCompleting(instrumentation,modules));
  }
 else {
    return this;
  }
}
