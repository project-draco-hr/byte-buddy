{
  return new StackManipulation.Compound(MethodInvocation.invoke(systemClassLoader),new TextConstant(Nexus.class.getName()),MethodInvocation.invoke(loadClass),new TextConstant("initialize"),ArrayFactory.targeting(TypeDescription.CLASS).withValues(Collections.singletonList(ClassConstant.of(TypeDescription.CLASS))),MethodInvocation.invoke(getDeclaredMethod),NullConstant.INSTANCE,ArrayFactory.targeting(TypeDescription.OBJECT).withValues(Collections.singletonList(ClassConstant.of(instrumentedType))),MethodInvocation.invoke(invokeMethod),Removal.SINGLE);
