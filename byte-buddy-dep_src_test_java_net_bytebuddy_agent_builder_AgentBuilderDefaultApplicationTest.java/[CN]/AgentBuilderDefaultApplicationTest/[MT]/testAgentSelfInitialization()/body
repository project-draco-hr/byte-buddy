{
  assertThat(ByteBuddyAgent.installOnOpenJDK(),instanceOf(Instrumentation.class));
  ClassFileTransformer classFileTransformer=new AgentBuilder.Default().rebase(isAnnotatedWith(ShouldRebase.class)).transform(new BarTransformer()).installOnByteBuddyAgent();
  try {
    assertThat(new Bar().foo(),is(BAR));
  }
  finally {
    ByteBuddyAgent.getInstrumentation().removeTransformer(classFileTransformer);
  }
}
