{
  ByteBuddyAgent.getInstrumentation().removeTransformer(new AgentBuilder.Default().with(poolStrategy).ignore(any()).with(AgentBuilder.InitializationStrategy.NoOp.INSTANCE).with(AgentBuilder.TypeStrategy.Default.REDEFINE).with(AgentBuilder.RedefinitionStrategy.REDEFINITION_CHUNKED).installOnByteBuddyAgent());
}
