{
  ClassFileTransformer classFileTransformer=new AgentBuilder.Default().disableSelfInitialization().rebase(isAnnotatedWith(ShouldRebase.class)).transform(new FooTransformer()).installOnByteBuddyAgent();
  try {
    assertThat(new Foo().foo(),is(BAR));
  }
  finally {
    ByteBuddyAgent.getInstrumentation().removeTransformer(classFileTransformer);
  }
}
