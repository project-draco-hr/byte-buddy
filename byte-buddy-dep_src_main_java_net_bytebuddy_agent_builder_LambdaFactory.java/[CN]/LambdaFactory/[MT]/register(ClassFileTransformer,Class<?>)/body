{
  try {
    @SuppressWarnings("unchecked") Map<ClassFileTransformer,LambdaFactory> classFileTransformers=(Map<ClassFileTransformer,LambdaFactory>)ClassLoader.getSystemClassLoader().loadClass(LambdaFactory.class.getName()).getDeclaredField("CLASS_FILE_TRANSFORMERS").get(null);
synchronized (classFileTransformers) {
      try {
        return classFileTransformers.isEmpty();
      }
  finally {
        classFileTransformers.put(classFileTransformer,new LambdaFactory(factory,factory.getDeclaredMethod("make",Object.class,String.class,Object.class,Object.class,Object.class,boolean.class,List.class,Collection.class)));
      }
    }
  }
 catch (  Exception exception) {
    throw new IllegalStateException("Could not register class file transformer",exception);
  }
}
