{
  if (type.isPrimitive()) {
    throw new IllegalArgumentException("Cannot subclass primitive type " + type);
  }
 else   if (type.isArray()) {
    throw new IllegalArgumentException("Cannot subclass array type " + type);
  }
 else   if (Modifier.isFinal(type.getModifiers())) {
    throw new IllegalArgumentException("Cannot subclass final type " + type);
  }
 else   if (type.isInterface()) {
    return new SubclassDynamicProxyBuilder(Object.class,Collections.<Class<?>>singletonList(type),ByteBuddy.DEFAULT_CLASS_VERSION,new NameMaker.PrefixingRandom(ByteBuddy.DEFAULT_NAME_PREFIX),ByteBuddy.DEFAULT_VISIBILITY,ByteBuddy.DEFAULT_MANIFESTATION,ByteBuddy.DEFAULT_FINAL_STATE,ByteBuddy.DEFAULT_SYNTHETIC_STATE,new Interception.Stack());
  }
 else {
    return new SubclassDynamicProxyBuilder(type,Collections.<Class<?>>emptyList(),ByteBuddy.DEFAULT_CLASS_VERSION,new NameMaker.PrefixingRandom(ByteBuddy.DEFAULT_NAME_PREFIX),ByteBuddy.DEFAULT_VISIBILITY,ByteBuddy.DEFAULT_MANIFESTATION,ByteBuddy.DEFAULT_FINAL_STATE,ByteBuddy.DEFAULT_SYNTHETIC_STATE,new Interception.Stack());
  }
}
