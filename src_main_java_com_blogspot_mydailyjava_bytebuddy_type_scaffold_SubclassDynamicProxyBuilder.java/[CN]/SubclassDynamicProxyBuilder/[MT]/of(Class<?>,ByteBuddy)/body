{
  if (type.isPrimitive()) {
    throw new IllegalArgumentException("Cannot subclass primitive type " + type);
  }
 else   if (type.isArray()) {
    throw new IllegalArgumentException("Cannot subclass array type " + type);
  }
 else   if (Modifier.isFinal(type.getModifiers())) {
    throw new IllegalArgumentException("Cannot subclass final type " + type);
  }
 else   if (type.isInterface()) {
    return new SubclassDynamicProxyBuilder(Object.class,Collections.<Class<?>>singletonList(type),byteBuddy.getClassVersion(),byteBuddy.getNameMaker(),byteBuddy.getVisibility(),byteBuddy.getTypeManifestation(),byteBuddy.getSyntheticState(),byteBuddy.getClassVisitorWrapperChain(),new MethodInterception.Stack());
  }
 else {
    return new SubclassDynamicProxyBuilder(type,Collections.<Class<?>>emptyList(),byteBuddy.getClassVersion(),byteBuddy.getNameMaker(),byteBuddy.getVisibility(),byteBuddy.getTypeManifestation(),byteBuddy.getSyntheticState(),byteBuddy.getClassVisitorWrapperChain(),new MethodInterception.Stack());
  }
}
