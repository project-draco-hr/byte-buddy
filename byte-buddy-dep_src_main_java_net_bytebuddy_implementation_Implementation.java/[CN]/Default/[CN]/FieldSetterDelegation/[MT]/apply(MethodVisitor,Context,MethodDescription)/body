{
  StackManipulation.Size stackSize=new StackManipulation.Compound(MethodVariableAccess.allArgumentsOf(instrumentedMethod.asDefined()).prependThisReference(),FieldAccess.forField(fieldDescription).putter(),MethodReturn.VOID).apply(methodVisitor,implementationContext);
  return new Size(stackSize.getMaximalSize(),instrumentedMethod.getStackSize());
}
