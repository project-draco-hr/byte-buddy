{
  StackManipulation.Size stackSize=new StackManipulation.Compound(fieldDescription.isStatic() ? StackManipulation.LegalTrivial.INSTANCE : MethodVariableAccess.REFERENCE.loadOffset(0),FieldAccess.forField(fieldDescription).getter(),MethodReturn.returning(fieldDescription.getType().asRawType())).apply(methodVisitor,implementationContext);
  return new Size(stackSize.getMaximalSize(),instrumentedMethod.getStackSize());
}
