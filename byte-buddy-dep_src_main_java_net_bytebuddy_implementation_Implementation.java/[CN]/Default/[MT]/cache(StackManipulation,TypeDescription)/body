{
  FieldCacheEntry fieldCacheEntry=new FieldCacheEntry(fieldValue,fieldType);
  FieldDescription fieldCache=registeredFieldCacheEntries.get(fieldCacheEntry);
  if (fieldCache != null) {
    return fieldCache;
  }
  validateFieldCacheAccessibility();
  fieldCache=new FieldDescription.Latent(instrumentedType,String.format("%s$%s",FIELD_CACHE_PREFIX,randomString.nextString()),fieldType,FIELD_CACHE_MODIFIER,Collections.<AnnotationDescription>emptyList());
  registeredFieldCacheEntries.put(fieldCacheEntry,fieldCache);
  return fieldCache;
}
