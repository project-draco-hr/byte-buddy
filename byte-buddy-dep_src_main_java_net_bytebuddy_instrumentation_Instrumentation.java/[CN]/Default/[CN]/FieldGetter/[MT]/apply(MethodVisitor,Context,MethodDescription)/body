{
  StackManipulation.Size stackSize=new StackManipulation.Compound(fieldDescription.isStatic() ? StackManipulation.LegalTrivial.INSTANCE : MethodVariableAccess.REFERENCE.loadFromIndex(0),FieldAccess.forField(fieldDescription).getter(),MethodReturn.returning(fieldDescription.getFieldType())).apply(methodVisitor,instrumentationContext);
  return new Size(stackSize.getMaximalSize(),instrumentedMethod.getStackSize());
}
