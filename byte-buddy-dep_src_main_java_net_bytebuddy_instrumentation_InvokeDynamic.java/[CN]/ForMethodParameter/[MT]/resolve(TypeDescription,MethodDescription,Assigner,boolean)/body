{
  TypeList parameterTypes=instrumentedMethod.getParameterTypes();
  if (index >= parameterTypes.size()) {
    throw new IllegalStateException("No parameter " + index + " for "+ instrumentedMethod);
  }
  return new Resolved.Simple(MethodVariableAccess.forType(parameterTypes.get(index)).loadFromIndex(instrumentedMethod.getParameterOffset(index)),parameterTypes.get(index));
}
