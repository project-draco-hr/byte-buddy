{
  ParameterList parameters=instrumentedMethod.getParameters();
  if (index >= parameters.size()) {
    throw new IllegalStateException("No parameter " + index + " for "+ instrumentedMethod);
  }
  return new Resolved.Simple(MethodVariableAccess.forType(parameters.get(index).getTypeDescription()).loadFromIndex(instrumentedMethod.getParameterOffset(index)),parameters.get(index).getTypeDescription());
}
