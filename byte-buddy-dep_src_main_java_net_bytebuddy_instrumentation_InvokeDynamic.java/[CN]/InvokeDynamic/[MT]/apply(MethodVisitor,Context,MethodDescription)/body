{
  StackManipulation.Size size=new StackManipulation.Compound(MethodVariableAccess.loadArguments(instrumentedMethod),MethodInvocation.invoke(bootstrapMethod).dynamic(instrumentedMethod.getInternalName(),instrumentedMethod.getReturnType(),instrumentedMethod.getParameterTypes(),arguments),MethodReturn.returning(instrumentedMethod.getReturnType())).apply(methodVisitor,instrumentationContext);
  return new Size(size.getMaximalSize(),instrumentedMethod.getStackSize());
}
