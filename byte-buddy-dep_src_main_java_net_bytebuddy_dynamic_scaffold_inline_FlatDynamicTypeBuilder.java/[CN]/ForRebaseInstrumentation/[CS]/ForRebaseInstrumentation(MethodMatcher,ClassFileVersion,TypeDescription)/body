{
  placeholderType=TrivialType.INSTANCE.make(trivialTypeNameFor(instrumentedType),classFileVersion,AuxiliaryType.MethodAccessorFactory.Illegal.INSTANCE);
  this.methodFlatteningResolver=new MethodFlatteningResolver.Default(ignoredMethods,placeholderType.getDescription(),new MethodFlatteningResolver.MethodNameTransformer.Suffixing(new RandomString()));
}
