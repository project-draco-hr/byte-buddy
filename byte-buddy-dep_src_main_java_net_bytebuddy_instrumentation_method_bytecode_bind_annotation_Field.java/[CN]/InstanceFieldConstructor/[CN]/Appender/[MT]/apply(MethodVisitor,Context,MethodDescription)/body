{
  StackManipulation.Size stackSize=new StackManipulation.Compound(MethodVariableAccess.REFERENCE.loadFromIndex(0),MethodInvocation.invoke(StaticFieldConstructor.INSTANCE.objectTypeDefaultConstructor),MethodVariableAccess.loadThisReferenceAndArguments(instrumentedMethod),FieldAccess.forField(fieldDescription).putter(),MethodReturn.VOID).apply(methodVisitor,instrumentationContext);
  return new Size(stackSize.getMaximalSize(),instrumentedMethod.getStackSize());
}
