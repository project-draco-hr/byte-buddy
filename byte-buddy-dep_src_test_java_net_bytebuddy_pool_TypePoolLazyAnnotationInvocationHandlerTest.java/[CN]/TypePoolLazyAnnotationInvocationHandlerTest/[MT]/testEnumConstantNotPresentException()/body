{
  when(annotationValue.load(getClass().getClassLoader())).thenThrow(new EnumConstantNotPresentException(Bar.class,FOO));
  new TypePool.LazyTypeDescription.AnnotationInvocationHandler(getClass().getClassLoader(),Foo.class,Collections.<String,TypePool.LazyTypeDescription.AnnotationValue<?,?>>singletonMap(FOO,annotationValue)).invoke(new Object(),Foo.class.getDeclaredMethod("foo"),new Object[0]);
}
