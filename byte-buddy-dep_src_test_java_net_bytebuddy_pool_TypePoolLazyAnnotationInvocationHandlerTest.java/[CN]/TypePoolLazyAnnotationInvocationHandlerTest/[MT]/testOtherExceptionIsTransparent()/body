{
  when(freeAnnotationValue.load(getClass().getClassLoader())).thenThrow(new RuntimeException());
  new TypePool.LazyTypeDescription.AnnotationInvocationHandler(getClass().getClassLoader(),Foo.class,Collections.<String,TypePool.LazyTypeDescription.AnnotationValue<?,?>>singletonMap(FOO,freeAnnotationValue)).invoke(new Object(),Foo.class.getDeclaredMethod("foo"),new Object[0]);
}
