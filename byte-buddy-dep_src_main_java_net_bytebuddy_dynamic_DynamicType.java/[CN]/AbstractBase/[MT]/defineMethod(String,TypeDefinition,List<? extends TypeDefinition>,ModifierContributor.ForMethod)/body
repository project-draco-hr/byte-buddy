{
  return new DefaultExceptionDeclarableMethodInterception(new MethodDescription.Token(isValidIdentifier(name),resolveModifierContributors(METHOD_MODIFIER_MASK,nonNull(modifier)),Collections.<TypeDescription.Generic>emptyList(),returnType.asGenericType(),new ParameterDescription.Token.TypeList(isActualType(parameterTypes)),Collections.<TypeDescription.Generic>emptyList(),Collections.<AnnotationDescription>emptyList(),null));
}
