{
  Assignment assignment=PrimitiveWideningDelegate.forPrimitive(new TypeDescription.ForLoadedType(sourceType)).widenTo(new TypeDescription.ForLoadedType(targetType));
  assertThat(assignment.isValid(),is(true));
  Assignment.Size size=assignment.apply(methodVisitor);
  assertThat(size.getSizeImpact(),is(0));
  assertThat(size.getMaximalSize(),is(0));
  verifyZeroInteractions(methodVisitor);
}
