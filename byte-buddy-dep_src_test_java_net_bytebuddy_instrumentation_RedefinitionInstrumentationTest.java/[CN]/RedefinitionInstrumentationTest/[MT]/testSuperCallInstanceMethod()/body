{
  Class<?> dynamicType=new ByteBuddy().redefine(Qux.class).name(FOO + RandomString.make()).method(named(BAR)).intercept(MethodDelegation.to(SuperInterceptor.class)).make().load(getClass().getClassLoader(),ClassLoadingStrategy.Default.WRAPPER).getLoaded();
  assertEquals(Bar.class,dynamicType.getSuperclass());
  Method barMethod=dynamicType.getDeclaredMethod(BAR);
  assertThat((String)barMethod.invoke(dynamicType.newInstance()),is(FOO + BAR));
}
