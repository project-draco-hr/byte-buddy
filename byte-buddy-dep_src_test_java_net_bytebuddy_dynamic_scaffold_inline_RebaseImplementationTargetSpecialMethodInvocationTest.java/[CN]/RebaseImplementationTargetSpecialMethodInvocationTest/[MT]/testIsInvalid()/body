{
  MethodRebaseResolver.Resolution resolution=mock(MethodRebaseResolver.Resolution.class);
  when(resolution.getAdditionalArguments()).thenReturn(StackManipulation.Illegal.INSTANCE);
  when(resolution.getResolvedMethod()).thenReturn(new MethodDescription.Latent(FOO,mock(TypeDescription.class),mock(TypeDescription.class),new TypeList.Empty(),Opcodes.ACC_PUBLIC,new TypeList.Empty()));
  Implementation.SpecialMethodInvocation specialMethodInvocation=new RebaseImplementationTarget.RebasedMethodSpecialMethodInvocation(resolution,mock(TypeDescription.class));
  assertThat(specialMethodInvocation.isValid(),is(false));
}
