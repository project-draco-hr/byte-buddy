{
  when(target.getParameterAnnotations()).thenReturn(new Annotation[0][0]);
  Iterator<Argument> iterator=Argument.NextUnboundAsDefaultsProvider.INSTANCE.makeIterator(typeDescription,source,target);
  assertThat(iterator.hasNext(),is(true));
  iterator.remove();
  assertThat(iterator.hasNext(),is(true));
  assertThat(iterator.next().value(),is(1));
  assertThat(iterator.hasNext(),is(false));
  iterator.remove();
}
