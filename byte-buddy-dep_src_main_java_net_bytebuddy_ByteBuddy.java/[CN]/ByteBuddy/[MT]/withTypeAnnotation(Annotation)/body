{
  return new ByteBuddy(classFormatVersion,namingStrategy,interfaceTypes,ignoredMethods,bridgeMethodResolverFactory,classVisitorWrapperChain,methodRegistry,modifiers,new Definable.Defined<TypeAttributeAppender>(new TypeAttributeAppender.ForAnnotation(nonNull(annotation))),defaultFieldAttributeAppenderFactory,defaultMethodAttributeAppenderFactory);
