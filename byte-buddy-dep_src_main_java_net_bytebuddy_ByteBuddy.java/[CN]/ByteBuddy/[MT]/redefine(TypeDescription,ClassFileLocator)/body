{
  return new RedefinitionDynamicTypeBuilder<T>(classFileVersion,namingStrategy.redefine(typeDescription),auxiliaryTypeNamingStrategy,implementationContextFactory,interfaceTypes,modifiers.resolve(typeDescription.getModifiers()),typeAttributeAppender,ignoredMethods,asmVisitorWrapper,new FieldRegistry.Default(),methodRegistry,methodGraphCompiler,defaultFieldAttributeAppenderFactory,defaultMethodAttributeAppenderFactory,typeDescription,classFileLocator);
}
