{
  FieldDescription valuesField=instrumentedType.getDeclaredFields().filter(named(ENUM_VALUES)).getOnly();
  MethodDescription cloneArrayMethod=new MethodDescription.Latent(CLONE_METHOD_NAME,valuesField.getFieldType(),TypeDescription.OBJECT,Collections.<TypeDescription>emptyList(),Opcodes.ACC_PUBLIC,Collections.<TypeDescription>emptyList());
  return new Size(new StackManipulation.Compound(FieldAccess.forField(valuesField).getter(),MethodInvocation.invoke(cloneArrayMethod),TypeCasting.to(valuesField.getFieldType()),MethodReturn.REFERENCE).apply(methodVisitor,implementationContext).getMaximalSize(),instrumentedMethod.getStackSize());
}
