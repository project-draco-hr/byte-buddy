{
  for (  String value : values) {
    instrumentedType=instrumentedType.withField(new FieldDescription.Token(value,ENUM_FIELD_MODIFIERS | Opcodes.ACC_ENUM,instrumentedType));
  }
  return instrumentedType.withField(new FieldDescription.Token(ENUM_VALUES,ENUM_FIELD_MODIFIERS | Opcodes.ACC_SYNTHETIC,TypeDescription.ArrayProjection.of(instrumentedType,1))).withInitializer(new InitializationAppender(values));
}
