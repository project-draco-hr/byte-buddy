{
  DynamicType.Loaded<?> loaded=implement(Object.class,MethodDelegation.to(Class.forName(DELEGATION_TARGET_EXPLICIT)),getClass().getClassLoader(),isMethod().and(not(isDeclaredBy(Object.class))),Class.forName(DEFAULT_INTERFACE));
  Object instance=loaded.getLoaded().getDeclaredConstructor().newInstance();
  assertThat(instance.getClass().getDeclaredMethod(FOO).invoke(instance),is((Object)(FOO + BAR)));
}
