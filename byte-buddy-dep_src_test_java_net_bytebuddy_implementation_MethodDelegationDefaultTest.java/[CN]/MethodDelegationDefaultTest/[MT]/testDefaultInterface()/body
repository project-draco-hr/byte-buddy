{
  DynamicType.Loaded<?> loaded=implement(Object.class,MethodDelegation.to(classLoader.loadClass(DELEGATION_TARGET)),classLoader,isMethod().and(not(isDeclaredBy(Object.class))),classLoader.loadClass(DEFAULT_INTERFACE));
  Object instance=loaded.getLoaded().newInstance();
  assertThat(instance.getClass().getDeclaredMethod(FOO).invoke(instance),is((Object)(FOO + BAR)));
}
