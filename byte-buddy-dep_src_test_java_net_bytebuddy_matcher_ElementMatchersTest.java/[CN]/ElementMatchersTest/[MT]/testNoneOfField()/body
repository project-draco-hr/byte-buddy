{
  assertThat(ElementMatchers.is(ElementMatchersTest.class.getDeclaredField("classLoader")).matches(new FieldDescription.ForLoadedField(ElementMatchersTest.class.getDeclaredField("classLoader"))),is(true));
  assertThat(ElementMatchers.is(ElementMatchersTest.class.getDeclaredField("classLoader")).matches(new FieldDescription.ForLoadedField(ElementMatchersTest.class.getDeclaredField("javaVersionRule"))),is(false));
}
