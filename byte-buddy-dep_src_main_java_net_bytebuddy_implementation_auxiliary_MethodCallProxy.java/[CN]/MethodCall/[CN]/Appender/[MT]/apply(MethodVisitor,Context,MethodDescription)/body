{
  StackManipulation thisReference=MethodVariableAccess.forType(instrumentedType).loadOffset(0);
  FieldList fieldList=instrumentedType.getDeclaredFields();
  StackManipulation[] fieldLoading=new StackManipulation[fieldList.size()];
  int index=0;
  for (  FieldDescription fieldDescription : fieldList) {
    fieldLoading[index++]=new StackManipulation.Compound(thisReference,FieldAccess.forField(fieldDescription).getter());
  }
  StackManipulation.Size stackSize=new StackManipulation.Compound(new StackManipulation.Compound(fieldLoading),MethodInvocation.invoke(accessorMethod),assigner.assign(accessorMethod.getReturnType(),instrumentedMethod.getReturnType(),true),MethodReturn.returning(instrumentedMethod.getReturnType())).apply(methodVisitor,instrumentationContext);
  return new Size(stackSize.getMaximalSize(),instrumentedMethod.getStackSize());
}
