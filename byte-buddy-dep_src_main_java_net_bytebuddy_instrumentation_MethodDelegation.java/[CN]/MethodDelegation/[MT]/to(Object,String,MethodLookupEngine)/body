{
  return new MethodDelegation(new InstrumentationDelegate.ForStaticFieldInstance(nonNull(delegate),isValidIdentifier(fieldName)),defaultParameterBinders(),defaultDefaultsProvider(),defaultAmbiguityResolver(),defaultAssigner(),methodLookupEngine.getReachableMethods(new TypeDescription.ForLoadedType(delegate.getClass())).filter(not(isStatic().or(isPrivate()).or(isConstructor()))));
}
