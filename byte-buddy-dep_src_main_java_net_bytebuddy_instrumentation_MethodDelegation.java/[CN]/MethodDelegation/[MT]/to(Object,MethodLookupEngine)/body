{
  return new MethodDelegation(new InstrumentationDelegate.ForStaticFieldInstance(nonNull(delegate)),defaultParameterBinders(),defaultDefaultsProvider(),defaultAmbiguityResolver(),defaultAssigner(),methodLookupEngine.process(new TypeDescription.ForLoadedType(delegate.getClass())).getInvokableMethods().filter(not(isStatic().or(isPrivate()).or(isConstructor()))));
}
