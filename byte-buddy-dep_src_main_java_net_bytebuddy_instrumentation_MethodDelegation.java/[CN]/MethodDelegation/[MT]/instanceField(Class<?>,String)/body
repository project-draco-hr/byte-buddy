{
  return new MethodDelegation(new InstrumentationDelegate.ForInstanceField(new TypeDescription.ForLoadedType(nonNull(type)),isValidIdentifier(fieldName)),defaultParameterBinders(),defaultDefaultsProvider(),defaultAmbiguityResolver(),defaultAssigner(),new TypeDescription.ForLoadedType(type).getReachableMethods().filter(not(isStatic().or(isPrivate()).or(isConstructor()))));
}
