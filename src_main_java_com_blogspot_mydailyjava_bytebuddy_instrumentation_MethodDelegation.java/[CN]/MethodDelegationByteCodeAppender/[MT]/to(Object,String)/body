{
  return new MethodDelegation(new InstrumentationDelegate.ForStaticFieldInstance(nonNull(delegate),isValidIdentifier(fieldName)),defaultArgumentBinders(),defaultDefaultsProvider(),defaultAmbiguityResolver(),defaultAssigner(),new TypeDescription.ForLoadedType(delegate.getClass()).getReachableMethods().filter(not(isStatic().or(isPrivate()).or(isConstructor()))));
}
