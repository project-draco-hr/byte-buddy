{
  return new MethodDelegation(new InstrumentationDelegate.ForInstanceField(new TypeDescription.ForLoadedType(nonNull(type)),isValidIdentifier(fieldName)),defaultArgumentBinders(),defaultDefaultsProvider(),defaultAmbiguityResolver(),defaultAssigner(),new TypeDescription.ForLoadedType(type).getReachableMethods().filter(not(isStatic().or(isPrivate()).or(isConstructor()))));
}
