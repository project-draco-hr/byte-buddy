{
  if (this == other)   return true;
  if (other == null || getClass() != other.getClass())   return false;
  ByConstructor that=(ByConstructor)other;
  return ignoreFinalizer == that.ignoreFinalizer && constructorParameters.equals(that.constructorParameters) && instrumentedType.equals(that.instrumentedType) && proxiedType.equals(that.proxiedType);
}
