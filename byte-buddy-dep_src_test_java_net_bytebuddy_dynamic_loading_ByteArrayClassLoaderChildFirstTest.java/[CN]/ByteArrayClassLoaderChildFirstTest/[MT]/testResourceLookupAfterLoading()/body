{
  assertThat(classLoader.loadClass(Foo.class.getName()).getClassLoader(),is(classLoader));
  InputStream inputStream=classLoader.getResourceAsStream(Foo.class.getName());
  try {
    assertThat(inputStream,expectedResourceLookup);
  }
  finally {
    if (inputStream != null) {
      inputStream.close();
    }
  }
}
