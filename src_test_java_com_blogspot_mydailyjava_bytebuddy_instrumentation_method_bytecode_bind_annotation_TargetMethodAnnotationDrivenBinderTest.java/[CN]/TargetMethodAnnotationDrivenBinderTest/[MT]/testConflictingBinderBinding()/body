{
  doReturn(FirstPseudoAnnotation.class).when(firstParameterBinder).getHandledType();
  doReturn(FirstPseudoAnnotation.class).when(secondParameterBinder).getHandledType();
  new TargetMethodAnnotationDrivenBinder(Arrays.<TargetMethodAnnotationDrivenBinder.ParameterBinder<?>>asList(firstParameterBinder,secondParameterBinder),defaultsProvider,assigner,methodInvoker);
}
