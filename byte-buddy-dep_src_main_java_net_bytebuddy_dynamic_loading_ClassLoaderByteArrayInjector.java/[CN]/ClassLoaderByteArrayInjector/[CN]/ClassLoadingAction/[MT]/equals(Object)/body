{
  if (this == other)   return true;
  if (other == null || getClass() != other.getClass())   return false;
  ClassLoadingAction that=(ClassLoadingAction)other;
  return Arrays.equals(binaryRepresentation,that.binaryRepresentation) && ClassLoaderByteArrayInjector.this.equals(that.getOuter()) && name.equals(that.name);
}
