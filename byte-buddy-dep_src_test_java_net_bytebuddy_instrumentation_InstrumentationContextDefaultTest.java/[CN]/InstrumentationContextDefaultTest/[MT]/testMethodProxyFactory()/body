{
  assertThat(defaultContext.requireAccessorMethodFor(firstMethod,AuxiliaryType.MethodAccessorFactory.LookupMode.Default.BY_SIGNATURE),is(firstProxyMethod));
  assertThat(defaultContext.requireAccessorMethodFor(secondMethod,AuxiliaryType.MethodAccessorFactory.LookupMode.Default.BY_SIGNATURE),is(secondProxyMethod));
  assertThat(defaultContext.requireAccessorMethodFor(firstMethod,AuxiliaryType.MethodAccessorFactory.LookupMode.Default.BY_SIGNATURE),is(firstProxyMethod));
  verify(methodAccessorFactory).requireAccessorMethodFor(firstMethod,AuxiliaryType.MethodAccessorFactory.LookupMode.Default.BY_SIGNATURE);
  verify(methodAccessorFactory).requireAccessorMethodFor(secondMethod,AuxiliaryType.MethodAccessorFactory.LookupMode.Default.BY_SIGNATURE);
  verifyNoMoreInteractions(methodAccessorFactory);
  verifyZeroInteractions(auxiliaryTypeNamingStrategy);
  verifyZeroInteractions(classFileVersion);
}
