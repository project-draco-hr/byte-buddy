{
  new TypeAttributeAppender.Explicit(new AnnotationList.ForLoadedAnnotation(new Baz.Instance())).apply(classVisitor,instrumentedType,valueFilter);
  verify(classVisitor).visitAnnotation(Type.getDescriptor(Baz.class),true);
  verifyZeroInteractions(classVisitor);
  verifyZeroInteractions(instrumentedType);
}
