{
  assertThat(describe(genericMethod).getReturnTypeGen(),is(GenericTypeDescription.Sort.describe(genericMethod.getGenericReturnType())));
  assertThat(describe(genericMethod).getParameters().asTypeListGen(),is((GenericTypeList)new GenericTypeList.ForLoadedType(genericMethod.getGenericParameterTypes())));
  assertThat(describe(genericMethod).getExceptionTypesGen(),is((GenericTypeList)new GenericTypeList.ForLoadedType(genericMethod.getGenericExceptionTypes())));
}
