{
  assertThat(describe(genericMethodWithRawException).getReturnType(),is(GenericTypeDescription.Sort.describe(genericMethodWithRawException.getGenericReturnType())));
  assertThat(describe(genericMethodWithRawException).getParameters().asTypeList(),is((GenericTypeList)new GenericTypeList.ForLoadedType(genericMethodWithRawException.getGenericParameterTypes())));
  assertThat(describe(genericMethodWithRawException).getExceptionTypes(),is((GenericTypeList)new GenericTypeList.ForLoadedType(genericMethodWithRawException.getGenericExceptionTypes())));
}
