{
  DynamicType.Loaded<?> loaded=instrument(Object.class,MethodDelegation.to(classLoader.loadClass(DELEGATION_TARGET)),classLoader,not(isDeclaredBy(Object.class)),classLoader.loadClass(DEFAULT_INTERFACE));
  Object instance=loaded.getLoaded().newInstance();
  assertThat(instance.getClass().getDeclaredMethod(FOO).invoke(instance),is((Object)(FOO + BAR)));
}
