{
  TypeDescription declaringType=typeDescription.getDeclaringType();
  return declaringType == null ? UNDEFINED : declaringType.isGenericDeclaration() ? new LazyParameterizedReceiverType(declaringType,typePath + GenericTypeToken.OWNER_TYPE_PATH) : new LazyNonGenericReceiverType(declaringType,typePath + GenericTypeToken.OWNER_TYPE_PATH);
}
