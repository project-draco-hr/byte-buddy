{
  Class<?> type=classLoader.loadClass(annotationToken.getDescriptor().substring(1,annotationToken.getDescriptor().length() - 1).replace('/','.'));
  if (type.isAnnotation()) {
    return new ForAnnotation.Loaded<Annotation>((Annotation)Proxy.newProxyInstance(classLoader,new Class<?>[]{type},AnnotationDescription.AnnotationInvocationHandler.of(classLoader,(Class<? extends Annotation>)type,annotationToken.getValues())));
  }
 else {
    return new ForAnnotation.IncompatibleRuntimeType(type);
  }
}
