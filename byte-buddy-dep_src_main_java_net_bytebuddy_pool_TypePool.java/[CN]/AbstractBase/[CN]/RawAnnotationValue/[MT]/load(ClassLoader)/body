{
  Class<?> type=classLoader.loadClass(annotationToken.getBinaryName());
  if (type.isAnnotation()) {
    return new ForAnnotation.Loaded<Annotation>(AnnotationDescription.AnnotationInvocationHandler.of(classLoader,(Class<? extends Annotation>)type,annotationToken.getValues()));
  }
 else {
    return new ForAnnotation.IncompatibleRuntimeType(type);
  }
}
