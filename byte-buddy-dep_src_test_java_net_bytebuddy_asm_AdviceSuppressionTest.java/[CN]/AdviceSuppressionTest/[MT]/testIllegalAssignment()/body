{
  Class<?> dynamicType=new ByteBuddy().redefine(Sample.class).visit(new AsmVisitorWrapper.ForDeclaredMethods().writerFlags(ClassWriter.COMPUTE_FRAMES).method(named(FOO),Advice.to(type))).make().load(null,ClassLoadingStrategy.Default.WRAPPER).getLoaded();
  assertThat(dynamicType.getDeclaredMethod(FOO).invoke(dynamicType.newInstance()),is((Object)FOO));
}
