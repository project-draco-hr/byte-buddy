{
  Class<?> dynamicType=new ByteBuddy().redefine(Sample.class).visit(DebuggingWrapper.makeDefault()).visit(Advice.to(type).on(named(FOO))).make().load(null,ClassLoadingStrategy.Default.WRAPPER).getLoaded();
  assertThat(dynamicType.getDeclaredMethod(FOO).invoke(dynamicType.newInstance()),is((Object)FOO));
}
