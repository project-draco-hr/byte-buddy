{
  this(instrumentedType,new FieldRegistry.Default(),new MethodRegistry.Default(),annotationRetention.isEnabled() ? new TypeAttributeAppender.ForInstrumentedType.Differentiating(originalType) : TypeAttributeAppender.ForInstrumentedType.INSTANCE,AsmVisitorWrapper.NoOp.INSTANCE,classFileVersion,auxiliaryTypeNamingStrategy,annotationValueFilterFactory,annotationRetention,implementationContextFactory,methodGraphCompiler,typeValidation,ignoredMethods,originalType,classFileLocator);
