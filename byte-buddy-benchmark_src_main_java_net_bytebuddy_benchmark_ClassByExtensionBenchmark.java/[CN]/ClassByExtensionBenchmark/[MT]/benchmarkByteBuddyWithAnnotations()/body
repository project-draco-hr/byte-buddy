{
  return new ByteBuddy().withIgnoredMethods(none()).subclass(baseClass).method(isDeclaredBy(ExampleClass.class)).intercept(MethodDelegation.to(ByteBuddyInterceptor.class)).make().load(newClassLoader(),ClassLoadingStrategy.Default.INJECTION.withPackageDefinitionStrategy(PackageDefinitionStrategy.NoOp.INSTANCE)).getLoaded().newInstance();
}
