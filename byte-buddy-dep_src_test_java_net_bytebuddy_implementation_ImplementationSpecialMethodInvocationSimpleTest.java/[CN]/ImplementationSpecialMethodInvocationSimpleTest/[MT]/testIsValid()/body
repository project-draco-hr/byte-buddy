{
  StackManipulation stackManipulation=mock(StackManipulation.class);
  when(stackManipulation.isValid()).thenReturn(true);
  Implementation.SpecialMethodInvocation specialMethodInvocation=new Implementation.SpecialMethodInvocation.Simple(new MethodDescription.Latent(FOO,mock(TypeDescription.class),mock(TypeDescription.class),Collections.<ParameterDescription.Token>emptyList(),Opcodes.ACC_PUBLIC,Collections.<GenericTypeDescription>emptyList(),Collections.<AnnotationDescription>emptyList()),mock(TypeDescription.class),stackManipulation);
  assertThat(specialMethodInvocation.isValid(),is(true));
}
