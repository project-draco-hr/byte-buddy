{
  super.visitCode();
  super.visitMethodInsn(Opcodes.INVOKESTATIC,"java/lang/Thread","currentThread","()Ljava/lang/Thread;",false);
  super.visitMethodInsn(Opcodes.INVOKEVIRTUAL,"java/lang/Thread","getId","()J",false);
  super.visitLdcInsn(id);
