{
  when(methodList.filter(isConstructor().<MethodDescription>and(isVisibleTo(instrumentedType)))).thenReturn((MethodList)filteredMethodList);
  assertThat(ConstructorStrategy.Default.IMITATE_SUPER_CLASS.extractConstructors(instrumentedType),is((List<MethodDescription.Token>)filteredMethodTokenList));
  assertThat(ConstructorStrategy.Default.IMITATE_SUPER_CLASS.inject(methodRegistry),is(methodRegistry));
  verify(methodRegistry).append(any(LatentMatcher.class),any(MethodRegistry.Handler.class),eq(MethodAttributeAppender.ForInstrumentedMethod.EXCLUDING_RECEIVER),eq(MethodTransformer.NoOp.INSTANCE));
  verifyNoMoreInteractions(methodRegistry);
  verify(instrumentedType,atLeastOnce()).getSuperClass();
  verifyNoMoreInteractions(instrumentedType);
}
