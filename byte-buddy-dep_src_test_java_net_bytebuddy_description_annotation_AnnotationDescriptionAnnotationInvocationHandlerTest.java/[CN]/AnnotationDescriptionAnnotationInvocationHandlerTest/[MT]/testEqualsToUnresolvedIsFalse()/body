{
  when(loadedAnnotationValue.getState()).thenReturn(AnnotationDescription.AnnotationValue.Loaded.State.NON_RESOLVED);
  assertThat(AnnotationDescription.AnnotationInvocationHandler.of(getClass().getClassLoader(),Foo.class,Collections.<String,AnnotationDescription.AnnotationValue<?,?>>singletonMap(FOO,annotationValue)).invoke(new Object(),Object.class.getDeclaredMethod("equals",Object.class),new Object[]{new ExplicitFoo(FOO)}),is((Object)false));
  verify(loadedAnnotationValue,never()).resolve();
}
