{
  when(loadedAnnotationValue.resolve()).thenReturn(new Object());
  AnnotationDescription.AnnotationInvocationHandler.of(getClass().getClassLoader(),Foo.class,Collections.<String,AnnotationDescription.AnnotationValue<?,?>>singletonMap(FOO,annotationValue)).invoke(new Object(),Foo.class.getDeclaredMethod("foo"),new Object[0]);
}
