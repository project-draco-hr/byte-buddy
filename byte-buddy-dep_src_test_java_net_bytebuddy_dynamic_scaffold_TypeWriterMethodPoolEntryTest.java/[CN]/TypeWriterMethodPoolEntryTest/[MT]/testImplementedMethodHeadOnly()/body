{
  when(parameterDescription.hasModifiers()).thenReturn(true);
  when(parameterDescription.isNamed()).thenReturn(true);
  TypeWriter.MethodPool.Entry entry=new TypeWriter.MethodPool.Entry.ForDeclaredMethod.WithBody(methodDescription,byteCodeAppender,methodAttributeAppender,modifierResolver);
  entry.applyHead(methodVisitor);
  verifyNoMoreInteractions(methodVisitor);
  verifyZeroInteractions(implementationContext);
  verifyZeroInteractions(methodAttributeAppender);
  verifyZeroInteractions(byteCodeAppender);
}
