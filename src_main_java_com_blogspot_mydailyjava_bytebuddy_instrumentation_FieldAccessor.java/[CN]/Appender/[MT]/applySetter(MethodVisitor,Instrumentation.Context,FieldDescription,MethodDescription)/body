{
  return apply(methodVisitor,instrumentationContext,fieldDescription,methodDescription,new StackManipulation.Compound(MethodVariableAccess.forType(fieldDescription.getFieldType()).loadFromIndex(methodDescription.getParameterOffset(0)),assigner.assign(methodDescription.getParameterTypes().get(0),fieldDescription.getFieldType(),considerRuntimeType),FieldAccess.forField(fieldDescription).putter()));
}
