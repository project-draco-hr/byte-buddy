{
  StackManipulation stackManipulation=new StackManipulation.Compound(ClassConstant.of(typeDescription),assigner.assign(new TypeDescription.ForLoadedType(Class.class),targetType,dynamicallyTyped));
  if (!stackManipulation.isValid()) {
    throw new IllegalStateException("Cannot assign Class value to " + targetType);
  }
  return stackManipulation;
}
