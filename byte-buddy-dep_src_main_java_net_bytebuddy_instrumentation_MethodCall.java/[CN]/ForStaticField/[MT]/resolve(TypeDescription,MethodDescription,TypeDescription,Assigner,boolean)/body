{
  StackManipulation stackManipulation=new StackManipulation.Compound(FieldAccess.forField(instrumentedType.getDeclaredFields().filter(named(fieldName)).getOnly()).getter(),assigner.assign(new TypeDescription.ForLoadedType(value.getClass()),targetType,dynamicallyTyped));
  if (!stackManipulation.isValid()) {
    throw new IllegalStateException("Cannot assign " + value.getClass() + " to "+ targetType);
  }
  return stackManipulation;
}
