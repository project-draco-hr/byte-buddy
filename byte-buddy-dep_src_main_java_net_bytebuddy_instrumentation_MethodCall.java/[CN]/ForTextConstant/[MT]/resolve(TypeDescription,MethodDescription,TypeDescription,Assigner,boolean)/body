{
  StackManipulation stackManipulation=new StackManipulation.Compound(new TextConstant(value),assigner.assign(new TypeDescription.ForLoadedType(String.class),targetType,dynamicallyTyped));
  if (!stackManipulation.isValid()) {
    throw new IllegalStateException("Cannot assign String value to " + targetType);
  }
  return stackManipulation;
}
