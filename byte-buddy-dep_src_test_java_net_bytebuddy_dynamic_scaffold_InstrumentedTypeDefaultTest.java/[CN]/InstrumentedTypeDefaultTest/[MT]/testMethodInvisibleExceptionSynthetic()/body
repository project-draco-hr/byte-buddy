{
  assertThat(makePlainInstrumentedType().withMethod(new MethodDescription.Token(FOO,Opcodes.ACC_SYNTHETIC,Collections.<TypeVariableToken>emptyList(),TypeDescription.Generic.OBJECT,Collections.<ParameterDescription.Token>emptyList(),Collections.singletonList(packagePrivateExceptionType),Collections.<AnnotationDescription>emptyList(),MethodDescription.NO_DEFAULT_VALUE)).validated(),notNullValue(TypeDescription.class));
}
