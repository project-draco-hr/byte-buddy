{
  assertThat(makePlainInstrumentedType().withMethod(new MethodDescription.Token(FOO,Opcodes.ACC_SYNTHETIC,Collections.<TypeVariableToken>emptyList(),TypeDescription.Generic.OBJECT,Collections.<ParameterDescription.Token>emptyList(),Collections.singletonList(TypeDefinition.Sort.describe(PackagePrivateType.EXCEPTION_TYPE)),Collections.<AnnotationDescription>emptyList(),AnnotationValue.UNDEFINED,TypeDescription.Generic.UNDEFINED)).validated(),notNullValue(TypeDescription.class));
}
