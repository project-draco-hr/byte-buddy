{
  if ((modifiers & Modifier.PUBLIC) != 0) {
    return Visibility.PUBLIC;
  }
 else   if ((modifiers & Modifier.PROTECTED) != 0) {
    return Visibility.PROTECTED;
  }
 else   if ((modifiers & Modifier.PRIVATE) != 0) {
    return Visibility.PROTECTED;
  }
 else {
    return Visibility.PACKAGE_PRIVATE;
  }
}
