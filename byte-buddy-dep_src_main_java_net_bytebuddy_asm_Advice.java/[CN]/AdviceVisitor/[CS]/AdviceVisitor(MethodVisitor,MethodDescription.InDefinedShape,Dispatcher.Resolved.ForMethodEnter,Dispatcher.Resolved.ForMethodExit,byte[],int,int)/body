{
  super(Opcodes.ASM5,methodVisitor);
  this.instrumentedMethod=instrumentedMethod;
  this.methodEnter=methodEnter;
  this.methodExit=methodExit;
  classReader=new ClassReader(binaryRepresentation);
  frameTranslator=FrameTranslator.Default.of(instrumentedMethod,methodEnter.getEnterType().represents(void.class) ? new TypeList.Empty() : new TypeList.Explicit(methodEnter.getEnterType()),writerFlags,readerFlags);
  endOfMethod=new Label();
}
