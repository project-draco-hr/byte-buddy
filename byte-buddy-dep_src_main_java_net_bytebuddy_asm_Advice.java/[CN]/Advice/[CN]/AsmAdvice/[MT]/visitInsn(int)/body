{
switch (opcode) {
case Opcodes.RETURN:
case Opcodes.IRETURN:
case Opcodes.FRETURN:
case Opcodes.DRETURN:
case Opcodes.LRETURN:
case Opcodes.ARETURN:
case Opcodes.ATHROW:
    classReader.accept(new CodeCopier(methodExit),ClassReader.SKIP_DEBUG);
  classReader.accept(new CodeCopier(methodExit),ClassReader.SKIP_DEBUG);
classReader.accept(new CodeCopier(methodExit),ClassReader.SKIP_DEBUG);
break;
}
super.visitInsn(opcode);
}
