{
  FrameTranslator.this.stackSize=Math.max(FrameTranslator.this.stackSize,stackSize);
  FrameTranslator.this.localVariableLength=Math.max(FrameTranslator.this.localVariableLength,localVariableLength + instrumentedMethod.getStackSize() + intermediateTypes.getStackSize() - methodDescription.getStackSize());
}
