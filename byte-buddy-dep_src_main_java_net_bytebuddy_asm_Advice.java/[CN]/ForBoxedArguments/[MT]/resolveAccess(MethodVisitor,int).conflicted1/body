{
switch (opcode) {
case Opcodes.ALOAD:
    methodVisitor.visitInsn(Opcodes.ACONST_NULL);
  break;
case Opcodes.ILOAD:
case Opcodes.LLOAD:
case Opcodes.FLOAD:
case Opcodes.DLOAD:
case Opcodes.ISTORE:
case Opcodes.FSTORE:
case Opcodes.ASTORE:
case Opcodes.LSTORE:
case Opcodes.DSTORE:
throw new IllegalStateException();
default :
throw new IllegalStateException("Unexpected opcode: " + opcode);
}
return NO_PADDING;
}
