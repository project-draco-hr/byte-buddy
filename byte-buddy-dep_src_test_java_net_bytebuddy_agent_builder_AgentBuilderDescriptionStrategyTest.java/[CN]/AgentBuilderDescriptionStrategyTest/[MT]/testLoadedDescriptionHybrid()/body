{
  assertThat(AgentBuilder.DescriptionStrategy.Default.HYBRID.apply(Object.class,typeLocator,locationStrategy,accessControlContext),is(TypeDescription.OBJECT));
  assertThat(AgentBuilder.DescriptionStrategy.Default.HYBRID.apply(Object.class,typeLocator,locationStrategy,accessControlContext),instanceOf(TypeDescription.ForLoadedType.class));
}
