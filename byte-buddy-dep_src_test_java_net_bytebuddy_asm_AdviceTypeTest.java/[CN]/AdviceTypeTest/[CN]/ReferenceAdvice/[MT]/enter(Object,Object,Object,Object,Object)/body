{
  if (value != null) {
    throw new AssertionError();
  }
  value=null;
  if (value != null) {
    throw new AssertionError();
  }
  if (!field.equals(FOO) || !mutated.equals(FOO) || !staticField.equals(FOO)|| !mutatedStatic.equals(FOO)) {
    throw new AssertionError();
  }
  mutated=mutatedStatic=BAR;
  enter++;
  return FOO + BAR;
}
