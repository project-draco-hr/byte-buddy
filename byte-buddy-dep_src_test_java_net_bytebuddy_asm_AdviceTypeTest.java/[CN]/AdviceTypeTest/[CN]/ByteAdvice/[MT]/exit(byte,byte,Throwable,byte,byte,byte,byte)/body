{
  if (result != (exception ? 0 : VALUE) || enter != VALUE * 2 || !(exception ? throwable instanceof RuntimeException : throwable == null)) {
    throw new AssertionError();
  }
  if (field != NUMERIC_DEFAULT || mutated != NUMERIC_DEFAULT * 2 || staticField != NUMERIC_DEFAULT || mutatedStatic != NUMERIC_DEFAULT) {
    throw new AssertionError();
  }
  exit++;
  return VALUE;
}
