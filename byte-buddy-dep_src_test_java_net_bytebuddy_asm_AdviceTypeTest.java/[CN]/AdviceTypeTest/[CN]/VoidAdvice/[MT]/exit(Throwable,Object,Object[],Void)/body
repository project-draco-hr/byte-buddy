{
  if (!(exception ? throwable instanceof RuntimeException : throwable == null)) {
    throw new AssertionError();
  }
  if (boxedReturn != null) {
    throw new AssertionError();
  }
  if (boxed.length != 2 || boxed[0] != null || boxed[1] != null) {
    throw new AssertionError();
  }
  if (custom != null) {
    throw new AssertionError();
  }
  exit++;
}
