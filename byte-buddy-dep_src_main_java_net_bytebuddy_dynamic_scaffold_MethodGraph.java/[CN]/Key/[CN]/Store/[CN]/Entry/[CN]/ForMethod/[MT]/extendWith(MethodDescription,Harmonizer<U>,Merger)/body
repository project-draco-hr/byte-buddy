{
  Harmonized<U> key=this.key.extend(methodDescription.asDefined(),harmonizer);
  return methodDescription.getDeclaringType().equals(this.methodDescription.getDeclaringType()) ? Ambiguous.of(key,methodDescription,this.methodDescription,merger) : new ForMethod<U>(key,methodDescription.isBridge() ? this.methodDescription : methodDescription,methodDescription.isBridge());
}
